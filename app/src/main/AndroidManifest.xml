<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gdgminna.android.gdgminna">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_gdgminna"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".OrganizersActivity"
            android:label="@string/activity_name_organizer" />
        <activity
            android:name=".WTMActivity"
            android:label="@string/activity_name_wtm" />
        <activity
            android:name=".EventsActivity"
            android:label="@string/activity_name_events" />
        <activity
            android:name=".DisclaimerActivity"
            android:label="@string/activity_name_disclaimer" />
        <activity
            android:name=".FAQActivity"
            android:label="@string/activity_name_faq" />
        <activity
            android:name=".RegisterActivity"
            android:label="@string/activity_name_register" />
        <activity
            android:name=".SocialmediaActivity"
            android:label="@string/activity_name_socialmedia" />
        <activity
            android:name=".AboutActivity"
            android:label="@string/activity_name_about" />
        <activity
            android:name=".FeedbackActivity"
            android:label="@string/activity_name_feedback" />
        <activity
            android:name=".ContactusActivity"
            android:label="@string/activity_name_contactus" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>